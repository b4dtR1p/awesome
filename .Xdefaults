URxvt.scrollBar: off
URxvt.font: xft:monaco:size=10

URxvt*termName:rxvt-unicode-256color
URxvt.jumpScroll: false
URxvt.saveLines: 32
URxvt.letterSpace: 0
URxvt.internalBorder: 0
URxvt*loginShell: true
URxvt.iconFile: /usr/share/icons/FlatWoken/scalable/apps/terminal.svg
Xft.dpi: 96
Xft.antialias: true
Xft.rgba: rgb
Xft.hinting: false
Xft.hinstyle: hintfull
Xft.lcdfilter: lcdlight
URxvt*xftAntialias: true

URxvt.keysym.C-3: command:\033]710;xft:monaco:size=6\007
URxvt.keysym.C-4: command:\033]710;xft:monaco:size=7\007
URxvt.keysym.C-5: command:\033]710;xft:monaco:size=8\007
URxvt.keysym.C-6: command:\033]710;xft:monaco:size=9\007
URxvt.keysym.C-7: command:\033]710;xft:monaco:size=11\007
URxvt.keysym.C-8: command:\033]710;xft:monaco:size=13\007
URxvt.keysym.C-9: command:\033]710;xft:monaco:size=17\007

urxvt.perl-ext-common: default,matcher,clipboard,url-select,keyboard-select
URxvt.background: [88]#12475B
URxvt.url-launcher: /usr/bin/firefox
URxvt.matcher.button: 1
URxvt.colorUL: #4682B4!
URxvt.depth: 32
!
URxvt.transparent: true
URxvt.shading: 30
!
URxvt.scrollBar: false
URxvt.scrollTtyOutput: false
URxvt.scrollWithBuffer: true
URxvt.scrollTtyKeypress: true
urxvt*secondaryScroll: true
urxvt*saveLines: 500
urxvt*urgentOnBell: false
!
URxvt*keysym.Home: \033[1~
URxvt*keysym.End: \033[4~

URxvt.keysym.Shift-Control-C : perl:clipboard:copy
URxvt.keysym.Shift-Control-V : perl:clipboard:paste
URxvt.iso14755: False
URxvt.url-select.launcher:  firefox
URxvt.url-select.underline: true
